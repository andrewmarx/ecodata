Package: ecodata
Type: Package
Title: Ecological Data
Version: 0.1.0
Authors@R: c(
    person("Andrew", "Marx", , "ajm.rpackages@gmail.com", role = c("aut", "cre", "cph"), 
      comment = c(ORCID = "0000-0002-7456-1631")
    )
